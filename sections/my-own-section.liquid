{% comment %}
  {%- liquid
    assign pro_list = section.settings.collection.products
    assign col = collections
  -%}
{% endcomment %}

<div class="page-width">
  {% comment %}
    <ul>
      {%- for pro in pro_list -%}
        <li>
          {{ pro.title }}
          {{ pro.price }}
          {{ pro.description }}
          {{ pro.featured_image | image_url: width: 400 | image_tag: alt: pro.title }}
        </li>
      {%- endfor -%}
    </ul>
  {% endcomment %}


  {% comment %} <ul>
    {%- for c in collections -%}
      <li>
        <h1>{{ c.title }}</h1>
        {{ c.all_products_count }}
        <br>
        {{ c.all_tags }}
        <br>
        {{ c.all_types }}
        <br>
        {{ c.all_vendors }}
        <br>
        {{ c.current_type }}
        <br>
        {{ c.default_sort_by }}
        <br>
        {{ c.description }}
        <br>
        {{ c.featured_image | image_url: width: 300, height:300 | image_tag: alt:c.title  }}
        <br>
        {{ c.handle }}
        <br>
        {{ c.id }}
        <br>
        {{ c.published_at | date }}
      </li>
    {%- endfor -%}
  </ul> {% endcomment %}
{% assign product = all_products['led-high-tops'] %}

{{ product.title }}
<br>
{{ product.available }}




</div>

{% schema %}
{
  "name": "Products List",
  "settings": [
    {
      "type": "collection",
      "id": "collection",
      "label": "Select a Collection"
    }
  ],
  "presets": [
    {
      "name": "Products List",
      "category": "Custom",
      "settings": {}
    }
  ]
}
{% endschema %}
